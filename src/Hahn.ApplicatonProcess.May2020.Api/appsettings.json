{
    "Serilog": {
      "MinimumLevel": "Debug",
      "WriteTo": [
        {
          "Name": "RollingFile",
          "Args": {
            "pathFormat": "C:\\Log\\ApplicationProcess-{Date}.txt",
            "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
          }
        }
      ]
    },
    "AllowedHosts": "*"
  }
